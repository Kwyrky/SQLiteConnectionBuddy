<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>SQLiteConnectionBuddy</id>
		<version>1.0.3</version>
		<title>SQLiteConnectionBuddy</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/SQLiteConnectionBuddy</projectUrl>
		<licenseUrl>https://github.com/dmanning23/SQLiteConnectionBuddy/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Simplify opening SQLite databases for cross-platform .Net projects</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>pcl, windows phone, winphone, android, ios, windows universal, monogame</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="SQLite.Net-PCL" version="3.1.1"/>
				<dependency id="SQLite.Net.Core-PCL" version="3.1.1"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="SQLite.Net-PCL" version="3.1.1"/>
				<dependency id="SQLite.Net.Core-PCL" version="3.1.1"/>
			</group>

			<!--Win Universal-->
			<group targetFramework="uap10.0">
				<dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="5.1"/>
				<dependency id="SQLite.Net-PCL" version="3.1.1"/>
			</group>

			<group targetFramework="net451">
				<dependency id="SQLite.Net-PCL" version="3.1.1"/>
				<dependency id="SQLite.Net.Core-PCL" version="3.1.1"/>
				<dependency id="System.Data.SQLite.Core" version="1.0"/>
			</group>

		</dependencies>
	</metadata>
	<files>

		<!--Xamarin.Android-->
		<file src="SQLiteConnectionBuddy.Android\bin\Release\SQLiteConnectionBuddy.Android.pdb" target="lib\MonoAndroid10\SQLiteConnectionBuddy.Android.pdb" />
		<file src="SQLiteConnectionBuddy.Android\bin\Release\SQLiteConnectionBuddy.Android.dll" target="lib\MonoAndroid10\SQLiteConnectionBuddy.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="SQLiteConnectionBuddy.iOS\bin\Release\SQLiteConnectionBuddy.iOS.pdb" target="lib\Xamarin.iOS10\SQLiteConnectionBuddy.iOS.pdb" />
		<file src="SQLiteConnectionBuddy.iOS\bin\Release\SQLiteConnectionBuddy.iOS.dll" target="lib\Xamarin.iOS10\SQLiteConnectionBuddy.iOS.dll" />

		<!--Windows Universal-->
		<file src="SQLiteConnectionBuddy.WindowsUniversal\bin\Release\SQLiteConnectionBuddy.WindowsUniversal.pdb" target="lib\uap10.0\SQLiteConnectionBuddy.WindowsUniversal.pdb" />
		<file src="SQLiteConnectionBuddy.WindowsUniversal\bin\Release\SQLiteConnectionBuddy.WindowsUniversal.dll" target="lib\uap10.0\SQLiteConnectionBuddy.WindowsUniversal.dll" />

		<!--DesktopGL-->
		<file src="SQLiteConnectionBuddy.DesktopGL\bin\Release\SQLiteConnectionBuddy.DesktopGL.pdb" target="lib\net451\SQLiteConnectionBuddy.DesktopGL.pdb" />
		<file src="SQLiteConnectionBuddy.DesktopGL\bin\Release\SQLiteConnectionBuddy.DesktopGL.dll" target="lib\net451\SQLiteConnectionBuddy.DesktopGL.dll" />

	</files>
</package>
